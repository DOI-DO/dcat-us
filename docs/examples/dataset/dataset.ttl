@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix org: <http://www.w3.org/ns/org#> .

# ---------- class Dataset --------------------------------------------------
<https://census.gov/dataset1>
  a dcat:Dataset ;

  # mandatory properties

  dcat:contactPoint [
    a vcard:Kind ;
    vcard:fn "US Census" ;
    vcard:email <mailto:noise@census.gov> 
  ], [
    a vcard:Kind ;
    vcard:fn "John Smith" ;
    vcard:email <mailto:johnsmith@census.gov> 
  ] ;
  dcterms:description 
                 """This dataset contains information regarding the
                 census in County of Loudoun"""@en ;
  dcterms:publisher <https://census.gov/> ;
  dcterms:title "Loudoun County"@en, "Condado de Loudoun"@es ;

  # recommended properties

  dcat:distribution <https://census.gov/data/123> ,
                    <https://census.gov/data/456> ;
  dcat:keyword "loudoun"@en,
               "census"@en;
            
  dcat:landingPage <https://opendata.census.gov/1234/about> ;
  dcterms:issued "2021-04-26"^^xsd:date ;
  dcterms:spatial <http://publications.europa.eu/resource/authority/country/CHE> ;
  dcterms:temporal [ a dcterms:PeriodOfTime ;
            dcat:endDate "2021-06-22T00:00:00"^^xsd:dateTime ;
            dcat:startDate "2020-01-01T00:00:00"^^xsd:dateTime ] ;
  dcat:theme <http://publications.europa.eu/resource/authority/data-theme/GOVE> ,
             <http://publications.europa.eu/resource/authority/data-theme/SOCI> ;
  dcterms:modified "2021-04-26"^^xsd:date .

# ---------- class Publisher ------------------------------------------------
<https://census.gov>
  a org:Organization ;
  foaf:name "US Census"@en .

# ---------- class Distribution ----------------------------------------------
<https://census.gov/data/123>
  a dcat:Distribution .

<https://census.gov/data/456>
  a dcat:Distribution .