@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix ex: <http://example.org/> .

# Define the Person (John Smith)
ex:JohnSmith a foaf:Person ;
    foaf:name "John Smith" ;
    foaf:firstName "John" ;
    foaf:givenName "Smith" ;
    org:memberOf <http://www.doi.gov> .

# Define the Organization (Department of Interior)
<http://www.doi.gov>  a org:Organization ;
    foaf:name "Department of Interior" ;
.
