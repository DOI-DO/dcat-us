:AccessRestriction_Shape-restrictionNote a sh:PropertyShape ;
    rdfs:label "restriction note" ;
    :requirementLevel :Optional ;
    sh:description "A note related to the access restriction" ;
    sh:maxCount 1 ;
    sh:minCount 0 ;
    sh:name "restrictionNote" ;
    sh:nodeKind sh:Literal ;
    sh:or dash:StringOrLangString ;
    sh:path dcat-us:restrictionNote ;
    sh:severity sh:Violation .

:AccessRestriction_Shape-restrictionStatus a sh:PropertyShape ;
    rdfs:label "restriction status" ;
    :requirementLevel :Mandatory ;
