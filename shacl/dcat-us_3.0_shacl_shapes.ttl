    sh:description "The indication of whether or not there are access restrictions on the data." ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "restrictionStatus" ;
    sh:nodeKind sh:IRI ;
    sh:path dcat-us:restrictionStatus ;
    sh:severity sh:Violation .

:AccessRestriction_Shape-specificRestriction a sh:PropertyShape ;
